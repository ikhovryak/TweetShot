{
    "manifest_version": 2,
    "name": "TweetShot",
    "description": "Fastest way to post your screenshots!" ,
    "version": "1.0",
    "icons": {"128": "icon_128.png"},
    "browser_action":{
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
	"content_scripts": [{
		"matches": [
			"<all_urls>"
		],
		"js": ["p5.js", "html2canvas.js", "script.js"]
	}]
}